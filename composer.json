{
  "name": "peibin/laravel-signal",
  "description": "Peibin signal component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "laravel",
    "signal"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "peibin/laravel-provider-config": "^1.0",
    "peibin/laravel-swoole-event": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Signal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Signal\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PeibinLaravel\\Signal\\SignalServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
